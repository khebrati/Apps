<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <title>@ViewData["Title"] - ExchangeBoardgamesApp</title>
    <link rel="stylesheet" href="~/css/_Layout.css" asp-append-version="true" />
</head>

<body style="cursor: default;">
        <nav>
        <img src="/images/icons/hashtag-c-sharp.svg" alt="c-sharp logo">
        <input type="search" placeholder="Give me something to search for you">
        <a class="icon">
            <img src="/images/icons/question-mark.svg" alt="help center">
        </a>
        <a class="icon">
            <img src="/images/icons/stack-exchange.svg" alt="list of all websites">
        </a>
            <a class="log-in button">
            Log in
        <div></div></a>
            <a class="sign-up button">
            Sign up
            </a>
    </nav>
    <div>
        <main>
            @RenderBody();
        </main>
    </div>



</body>

</html>